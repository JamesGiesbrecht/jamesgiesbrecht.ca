{
  "name": "next-client",
  "private": true,
  "scripts": {
    "browser": "open -a \"Google Chrome\" http://localhost:3001/",
    "build": "yarn build:next && yarn build:server",
    "build:next": "next build",
    "build:server": "tsc --project tsconfig.server.json",
    "build:server:dev": "tsc --project tsconfig.server.json --watch --preserveWatchOutput",
    "start:next": "next start",
    "start:server": "concurrently \"yarn build:server:dev\" \"nodemon dist/server/index.js\"",
    "dev": "yarn build:server && yarn start:server",
    "start": "NODE_ENV=production node dist/server/index.js",
    "compose:dev": "docker-compose -f docker-compose.yaml -f docker-compose.dev.yaml up -d --build && yarn browser && yarn compose:logs",
    "compose:prod": "docker-compose up -d --build && yarn browser && yarn compose:logs",
    "compose:logs": "docker-compose logs -f",
    "lint": "eslint '*/**/*.{js,jsx,ts,tsx}'"
  },
  "dependencies": {
    "@emotion/cache": "^11.7.1",
    "@emotion/react": "^11.7.1",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.4.2",
    "@mui/lab": "^5.0.0-alpha.69",
    "@mui/material": "^5.4.2",
    "@mui/styles": "^5.4.2",
    "axios": "^0.26.0",
    "body-parser": "^1.19.2",
    "cheerio": "^1.0.0-rc.10",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "firebase": "^9.6.6",
    "firebase-admin": "^10.0.2",
    "gray-matter": "^4.0.3",
    "markdown-it": "^13.0.1",
    "mongoose": "^6.2.5",
    "next": "12.0.10",
    "notistack": "^2.0.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-masonry-css": "^1.0.16",
    "react-reveal": "^1.2.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-serve-static-core": "^4.17.28",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^17.0.21",
    "@types/react": "17.0.39",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "babel-plugin-inline-react-svg": "^2.0.1",
    "concurrently": "^7.0.0",
    "eslint": "8.9.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "12.0.10",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-security": "^1.4.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "typescript": "4.5.5"
  }
}
