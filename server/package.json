    "@types/express": "^4.17.13",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-security": "^1.4.0",
    "nodemon": "^2.0.6",
    "prettier": "^2.4.0",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "axios": "^0.21.1",
